  "@type": "sc:Dataset",
  "name": "AssetOpsBench-scenarios",
  "description": "AssetOpsBench is a unified framework and environment designed to guide the development, orchestration, and evaluation of domain-specific agents for task automation in industrial asset operations and maintenance. The release of the benchmark focuses on scenarios commonly posed by domain experts\u2014such as maintenance engineers, reliability specialists, and facility planners. We devloped 4 individual domain-specific agents and 2 multi-agent orchestration frameworks to create a simulated industrial environment enabling end-to-end benchmarking of multi-agent workflows in asset operations.",
  "conformsTo": "http://mlcommons.org/croissant/1.0",
  "url": "https://github.com/IBM/AssetOpsBench",
  "distribution": [
    {
      "@type": "cr:FileObject",
      "@id": "AssetOpsBench",
      "name": "AssetOpsBench",
      "description": "AssetOpsBench datasets",
      "contentUrl": "https://github.com/IBM/AssetOpsBench",
      "encodingFormat": "git+https",
      "sha256": "main"
    },
    {
      "@type": "cr:FileSet",
      "@id": "AssetOpsBench-scenario-files",
      "name": "AssetOpsBench-scenario-files",
      "description": "JSONL-format AssetOpsBench datasets.",
      "containedIn": {
        "@id": "AssetOpsBench"
      },
      "encodingFormat": "application/jsonlines",
      "includes": "scenarios/all_utterance.jsonl"
    }
  ],
  "recordSet": [
    {
      "@type": "cr:RecordSet",
      "@id": "AssetOpsBench-scenario-records",
      "name": "AssetOpsBench-scenario-records",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "jsonl/id",
          "name": "id",
          "description": "id of utterance",
          "dataType": "sc:Integer",
          "source": {
            "fileSet": {
              "@id": "AssetOpsBench-scenario-files"
            },
            "extract": {
              "column": "id"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "jsonl/type",
          "name": "type",
          "description": "the domain type of utterance, e.g. IoT, FMSR.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "AssetOpsBench-scenario-files"
            },
            "extract": {
              "column": "type"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "jsonl/category",
          "name": "category",
          "description": "the category of the query, e.g. Knowledge Query, Tuning Query.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "AssetOpsBench-scenario-files"
            },
            "extract": {
              "column": "category"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "jsonl/text",
          "name": "text",
          "description": "the task description of the utterance",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "AssetOpsBench-scenario-files"
            },
            "extract": {
              "column": "text"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "jsonl/characteristic_form",
          "name": "characteristic_form",
          "description": "the characteristics of the expected output",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "AssetOpsBench-scenario-files"
            },
            "extract": {
              "column": "characteristic_form"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "jsonl/deterministic",
          "name": "deterministic",
          "description": "is the expected output deterministic?",
          "dataType": "sc:Boolean",
          "source": {
            "fileSet": {
              "@id": "AssetOpsBench-scenario-files"
            },
            "extract": {
              "column": "deterministic"
            }
          }
        }
      ]
    }
  ]
}
